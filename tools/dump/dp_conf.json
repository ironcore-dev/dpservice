{
  "header": "opts.h",
  "source": "opts.c",
  "markdown": "../../docs/deployment/help_dpservice-dump.md",
  "options": [
    {
      "lgopt": "drops",
      "help": "show dropped packets",
      "var": "showing_drops",
      "type": "bool",
      "default": "false"
    },
    {
      "lgopt": "nodes",
      "help": "show graph node traversal",
      "var": "showing_nodes",
      "type": "bool",
      "default": "false"
    },
    {
      "lgopt": "node-filter",
      "help": "show only nodes with name matching REGEX",
      "arg": "REGEX"
    },
    {
      "lgopt": "filter",
      "help": "show only packets matching a pcap-style FILTER",
      "arg": "FILTER"
    },
    {
      "lgopt": "hw",
      "help": "capture offloaded packets (only outgoing VF->PF packets supported)",
      "var": "offload_enabled",
      "type": "bool",
      "default": "false"
    },
    {
      "lgopt": "pcap",
      "help": "write packets into a PCAP file",
      "arg": "FILE"
    },
    {
      "lgopt": "stop",
      "help": "do nothing, only make sure tracing is disabled in dp-service",
      "var": "stop_mode",
      "type": "bool",
      "default": "false"
    }
  ]
}
